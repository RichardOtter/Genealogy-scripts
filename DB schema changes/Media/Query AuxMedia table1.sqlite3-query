-- database: ./TEST-DB schema changes.rmtree

Select datetime(TimeCreation + 2415018.5, 'localtime'), datetime(amt.UTCModDate + 2415018.5, 'localtime'), datetime(TimeModification + 2415018.5, 'localtime'),*
from MultiMediaTable
Join  AuxMultimediaTable as amt on MediaID = AuxMediaID
order by TimeCreation asc
